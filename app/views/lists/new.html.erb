<div class="container mt-4">
  <div class="row">
    <div class="col-6 m-auto bg-light">
      <div class="bookmark-form ml-3 mr-3 mt-3 mb-4">
        <h1>New movie list</h1>
        <div class="mt-3">
          <%= simple_form_for(@list) do |f| %>
            <%= f.input :name %>
            <%= f.input :photo, as: :file %>
            <%= f.submit %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
